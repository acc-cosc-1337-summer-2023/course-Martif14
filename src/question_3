
#include <iostream>
using namespace std;


int get_fib_sequence() 
{
	int counter=0;
    int numbers[4] = {1,1};
	for (int i = 0; i < 4; i++) {
		numbers[i] = numbers[i - 1] + numbers[i - 2];
	}
	cout << "The first 4 elements:\n";
	for (int i = 0; i < 4; i++) {
		cout << numbers[i] << ", ";
	}
	cout << numbers[4];
	cin>>counter;
    return 0;
}